{
  "name": "execut/yii2-crud",
  "description": "Yii2 object for fasted configuration CRUD navigation and actions for models without copy-paste or CRUD generators",
  "keywords": ["yii2", "crud"],
  "homepage": "https://github.com/execut/yii2-crud",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/execut/yii2-crud/issues?state=open",
    "wiki": "https://github.com/execut/yii2-crud/wiki",
    "source": "https://github.com/execut/yii2-crud"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "yiisoft/yii2": "2.*",
    "execut/yii2-base": "*",
    "execut/yii2-navigation": "*",
    "execut/yii2-actions": "*"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0"
  },
  "autoload": {
    "psr-4": {
      "execut\\crud\\": "src"
    }
  }
}
